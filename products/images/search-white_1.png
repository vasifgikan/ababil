    //Common js code
    /* Mobile Navigation */
        $('.mnav-trigger').click(function() {
            $('body').addClass('show-mnav');
        });
        $('.mnav-clstrigger').click(function() {
            $('body').removeClass('show-mnav');
        });

        $(".head-search").click(function(){
            $(".search-form").toggle();
        })
        $(window).scroll(function() {
        if ($(this).scrollTop() > 1){  
            $('header').addClass("sticky");
        }
        else{
            $('header').removeClass("sticky");
        }
        });


        // quote form
        $(".get-quote").click(function(){
            $('.quote-form').addClass('show-quote-form'); 
            e.stopPropagation();
            e.preventDefault();

        });


        $(".close-quote-form").click(function(){
            $('.quote-form').removeClass('show-quote-form');

        });

        $(document).mouseup(function(e) 
                            {
            if (!$(e.target).closest(".quote-form .quoteform-innr").length) 
            {
            if($('.quote-form').hasClass('show-quote-form') ) {

                $('.quote-form').removeClass('show-quote-form');
            }
            }    
        });


        $(".nwsltr-btn").click(function (e) {
        $(".subscribe-popup-container").addClass("show-popup");
        })
        $(".subscribe-popup-close").click(function (){
            $(".subscribe-popup-container").removeClass("show-popup");
        })

        $(document).mouseup(function (e) {
            if (!$(e.target).closest(".subscribe-popup-container form").length) {
                if ($('.subscribe-popup-container').hasClass('show-popup')) {

                    $('.subscribe-popup-container').removeClass('show-popup');
                }
            }
        });


        // Enquiry FormData
        $(".inquire-btn,.apply-btn").click(function (e) {
                $('.inquire-form-container').addClass('show-inquire-form');
                e.stopPropagation();
                e.preventDefault();
                
                console.log("1");
            });
            $(".close-inquire-form").click(function () {
                $('.inquire-form-container').removeClass('show-inquire-form');
                console.log("2");


            });



       
